#include <stdio.h>
#include <time.h>

int main() {
	//variable for time calculation
	clock_t start, end;
	double cpu_time_used;
	
	//using for loop
	printf("Using for-loop!");
	start = clock();
	
	for ( i=100, i>=0, i--)
	{
		printf("%d ", i);
	}
	end = clock();
	cpu_time_used = ((double) (end - start)) / CLOCKS_PER_SEC;
	

}

